//  Program to find a largest of the element 

#include <iostream>
using namespace std;
int main()
{
  int arr [10] = {41,566,496,254,251,475,968,2563,48,856};
  int max = arr[0];
  for (int i = 0; i < 10; i++)
  {
      if(arr[i] > max ) {
          max = arr[i];
      }
  }
  cout << "the largest number is : " << max;
    return 0;
}
