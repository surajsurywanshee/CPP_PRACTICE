// SUM OF THE GIVEN DIGITS

#include <iostream>
using namespace std;
int main()
{
    int num, sum = 0;
     cout << "Enter the number :" ;
     cin >> num;
     
     for(int i = 1; i <= num; i++) {
         sum = sum + i;
     }
        cout<<"Sum of to all digits to giving that :"<< sum;

    return 0;
}
